{
	"env": {
		"es2021": true,
		"node": true
	},
	"extends": "standard",
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": {
		"indent": ["error", "tab"],
		"no-tabs": "off",
		"space-before-function-paren": [
			"error",
			{
				"named": "never"
			}
		],
		"camelcase": "off",
		"comma-dangle": [
			"error",
			{
				"imports": "always-multiline",
				"arrays": "always-multiline",
				"objects": "always-multiline"
			}
		],
		"semi": ["error", "always"],
		"no-unreachable": "warn"
	}
}
